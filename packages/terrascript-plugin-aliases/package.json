{
  "name": "@joshwycuff/terrascript-plugin-aliases",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "Josh Wycuff <Joshua.Wycuff@turner.com>",
  "homepage": "",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "yarn build",
    "yalc:publish": "yalc publish --push",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "watch": "npm-watch build"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts",
      "verbose": true,
      "runOnChangeOnly": true
    }
  },
  "directories": {
    "lib": "src",
    "test": "src/__tests__"
  },
  "files": [
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshwycuff/terrascript.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "yarn.lock",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
      }
    ],
    "extends": "semantic-release-monorepo"
  },
  "dependencies": {
    "@joshwycuff/config": "^0.0.0",
    "@joshwycuff/terrascript-core": "0.0.0",
    "@joshwycuff/types": "0.0.0",
    "lodash": "^4.17.21",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.8",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.6.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-unicorn": "^22.0.0",
    "jest": "^26.4.2",
    "npm-watch": "^0.9.0",
    "prettier": "^2.1.2",
    "semantic-release": "^17.4.3",
    "semantic-release-monorepo": "^7.0.5",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3"
  }
}
